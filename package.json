{
	"name": "should-semantic-release",
	"version": "0.3.5",
	"description": "Checks whether a semantic release should be run for a repository. ðŸ’‚",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/should-semantic-release.git"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"type": "module",
	"main": "lib/index.js",
	"bin": "bin/should-semantic-release.mjs",
	"files": [
		"LICENSE.md",
		"README.md",
		"bin/should-semantic-release.mjs",
		"lib/",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
